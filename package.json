{
  "name": "ynab-mcp-server",
  "version": "0.1.2",
  "description": "ynab-mcp-server MCP server",
  "author": "Caleb LeNoir <caleb.lenoir@hey.com>",
  "type": "module",
  "module": "src/index.ts",
  "bin": {
    "ynab-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx @smithery/cli dev",
    "build": "npm run build:http",
    "build:stdio": "tsc",
    "build:http": "npx @smithery/cli build",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "npm run start:http",
    "start:stdio": "node dist/index.js",
    "start:http": "node .smithery/index.cjs",
    "debug": "npm run build && npx @modelcontextprotocol/inspector dist/index.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/axios": "^0.14.4",
    "axios": "^1.8.4",
    "mcp-framework": "^0.2.15",
    "ynab": "^2.9.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.16.5",
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
